<!DOCTYPE html>
<html>
<%- include ./../public/header %>

    <body>
        <div class="wrapper">
            <%- include ./../public/sidebar %>
                <div class="main-panel">
                    <%- include ./../public/nav %>
                        <div class="content" style="margin-top: 5px;">
                            <div class="investment_f">
                                <div class="investment_con">
                                    <form class="form-inline querybox" action="#">
                                        <div class="form-group">
                                            <label>XXXXXXXX：</label>
                                            <input class="form-input" name="name" />
                                        </div>
                                        <button type="submit" class="btn btn-success btn-query">
                            <span class="glyphicon glyphicon-search btn_icon" aria-hidden="true">查询</span>
                        </button>
                                    </form>

                                    <div class="card card-box">
                                        <div class="list-header form-inline">
                                            <h4 class="title"><strong>列表</strong></h4>
                                            <div class="text-right handle-box">
                                                <a href="#" type="button" class="btn btn-success btn-sm">
                                    <span class="glyphicon glyphicon-plus" aria-hidden="true">新增</span>
                                </a>
                                            </div>
                                        </div>
                                        <div class="card-content table-responsive">
                                            <table class="table table-box table-condensed table-bordered">
                                                <thead class="text-primary">
                                                    <tr>
                                                        <th>用户id</th>
                                                        <th>交易订单号</th>
                                                        <th>交易金额</th>
                                                        <th>交易类型（参照类型表）</th>
                                                        <th>创建时间</th>
                                                        <th>创建日期</th>
                                                        <th>手续费 元</th>
                                                        <th>手续费 %</th>
                                                        <th>交易状态 1成功，0失败 2待确认 3,批量资金发放，待输入密码</th>
                                                        <th>字段11</th>
                                                        <th>操作次数</th>
                                                        <th>操作对应的transferBank的银行卡号</th>
                                                        <th>目标用户id</th>
                                                        <th>渠道回调url</th>
                                                        <th>渠道订单号</th>
                                                        <th>批量资金发放批次号</th>
                                                        <th>渠道唯一标识</th>
                                                        <th>订单备注</th>
                                                        <th>渠道批量资金发放批次号</th>
                                                        <th>渠道批量资金发放回调地址</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% !!locals.list &&list.rows.forEach((item, index)=>{ %>
                                                        <tr>
                                                            <td>
                                                                <%= item.userId%>
                                                            </td>
                                                            <td>
                                                                <%= item.transferRequestNo%>
                                                            </td>
                                                            <td>
                                                                <%= item.amount%>
                                                            </td>
                                                            <td>
                                                                <%= item.tradeType%>
                                                            </td>
                                                            <td>
                                                                <%= item.creatTime%>
                                                            </td>
                                                            <td>
                                                                <%= item.creatMonth%>
                                                            </td>
                                                            <td>
                                                                <%= item.tradeCharge%>
                                                            </td>
                                                            <td>
                                                                <%= item.tradeChargeRate%>
                                                            </td>
                                                            <td>
                                                                <%= item.status%>
                                                            </td>
                                                            <td>
                                                                <%= item.statusDesc%>
                                                            </td>
                                                            <td>
                                                                <%= item.operateCount%>
                                                            </td>
                                                            <td>
                                                                <%= item.operateBankCardId%>
                                                            </td>
                                                            <td>
                                                                <%= item.targetUserId%>
                                                            </td>
                                                            <td>
                                                                <%= item.customBackUrl%>
                                                            </td>
                                                            <td>
                                                                <%= item.customOrderNo%>
                                                            </td>
                                                            <td>
                                                                <%= item.batchNo%>
                                                            </td>
                                                            <td>
                                                                <%= item.merchantId%>
                                                            </td>
                                                            <td>
                                                                <%= item.historyDesc%>
                                                            </td>
                                                            <td>
                                                                <%= item.customBatchNo%>
                                                            </td>
                                                            <td>
                                                                <%= item.batchOutCallBackUrl%>
                                                            </td>
                                                        </tr>
                                                        <% }) %>
                                                </tbody>
                                            </table>
                                            <div class="tcdPageCode" style="padding-top: 10px"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                </div>
        </div>
    </body>

</html>