                                                <input type="text" name="company_id" value="<%= locals.data && data.company_id%>" required>
                                            </div>
                                            <div class="col-md-5">
                                                <label>营业执照</label>
                                                <input type="text" name="g9" value="<%= locals.data && data.g9%>" required>
