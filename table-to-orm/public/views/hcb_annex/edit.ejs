                                            <div class="col-md-5 col-md-offset-1">
                                                <label>企业征信报告</label>
                                                <input type="text" name="L10" value="<%= locals.data && data.L10%>" required>
                                            </div>
                                            <div class="col-md-5">
                                                <label>法人及实际控制人征信报告</label>
                                                <input type="text" name="L11" value="<%= locals.data && data.L11%>" required>
                                            </div>
                                        </div>
