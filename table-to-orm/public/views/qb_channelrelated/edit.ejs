<!DOCTYPE html>
<html>
<%- include ./../public/header %>

    <body>
        <div class="wrapper">
            <%- include ./../public/sidebar %>
                <div class="main-panel">
                    <%- include ./../public/nav %>
                        <div class="content" style="margin-top: 5px;">
                            <div style="padding: 14px">
                                <form action="#" method="post" class="form-check-rule edit_box">
                                    <div class="text-center bg-primary edit_title">
                                        <h4>修改信息</h4>
                                    </div>
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-5 col-md-offset-1">
                                                <label>商户标识（原公司）</label>
                                                <input type="text" name="originalId" value="<%= locals.data && data.originalId%>" required>
                                            </div>
                                            <div class="col-md-5">
                                                <label>关联的佣金待发商户（佣金代发公司）</label>
                                                <input type="text" name="companyId" value="<%= locals.data && data.companyId%>" required>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5 col-md-offset-1">
                                                <label>字段4</label>
                                                <input type="text" name="profiltUpper" value="<%= locals.data && data.profiltUpper%>" required>
                                            </div>
                                            <div class="col-md-5">
                                                <label>字段5</label>
                                                <input type="text" name="profiltLower" value="<%= locals.data && data.profiltLower%>" required>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5 col-md-offset-1">
                                                <label>平台标识</label>
                                                <input type="text" name="merchantId" value="<%= locals.data && data.merchantId%>" required>
                                            </div>
                                            <div class="col-md-5">
                                                <label>关联状态：0 无效  1 目前 启用      2  历史</label>
                                                <input type="text" name="status" value="<%= locals.data && data.status%>" required>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5 col-md-offset-1">
                                                <label>创建时间</label>
                                                <input type="text" name="createtime" value="<%= locals.data && data.createtime%>" required>
                                            </div>
                                            <div class="col-md-5">
                                                <label>修改时间</label>
                                                <input type="text" name="updatetime" value="<%= locals.data && data.updatetime%>" required>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5 col-md-offset-1">
                                                <label>服务费</label>
                                                <input type="text" name="serviceRates" value="<%= locals.data && data.serviceRates%>" required>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row" style="text-align: center;">
                                        <button class="btn btn-success" type="submit">添加</button>
                                        <a href="#" class="btn btn-success">返回</a>
                                    </div>
                                </form>
                            </div>
                        </div>
                </div>
        </div>
    </body>

</html>