.container {
    background-color: #F9F9F9;
}
.border {
    border:solid 1px #E4E4E4;
    background:#fff;
    height:60px;line-height:60px;
}
.button button{
    border-radius: 10px;
	color:#f8d748;
}
.footer {
	width: 100%;
	height:40px;
	line-height:40px;
	flex-direction: row;
	position: fixed;
	top: 0;
}
.voucher{
	border-radius: 20px;
	height:75px;margin:10px auto;
	color: #f8d748;
}
.voucher .top{
	height: 15px;
	border-radius:20px 20px 0 0;
	background-color: #f8d748;
}
.content{
	padding:0 10px;
	height: 60px;
	line-height: 60px;
}